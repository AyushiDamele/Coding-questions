#include<bits/stdc++.h>
using namespace std;
void split(string &s, char delim) {
        stringstream ss(s);
        string item;
        while (getline(ss,item,delim)) {
            cout << item<<endl;
        }
    }
int main()
{
    string s;
    getline(cin,s);
    split(s,'.');
    return 0;
}

